{
  "name": "@newdash/inject",
  "version": "0.2.30",
  "main": "lib/index.js",
  "license": "MIT",
  "description": "typescript dependency inject container",
  "scripts": {
    "prebuild": "rimraf lib",
    "pretest": "eslint test/**/*.ts --fix",
    "test": "jest --detectOpenHandles --passWithNoTests",
    "build": "tsc",
    "lint": "eslint src/** --fix",
    "release": "npx standard-version --no-verify && git push --follow-tags origin main",
    "coverage": "npm test -- --coverage --no-cache"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/newdash/inject"
  },
  "engines": {
    "node": ">=10"
  },
  "dependencies": {
    "@newdash/newdash": "^5.20.0",
    "debug": "^4.3.3",
    "@newdash/graphlib": "^3",
    "reflect-metadata": "^0.1.13"
  },
  "devDependencies": {
    "@commitlint/config-conventional": "^15",
    "@types/debug": "^4.1.7",
    "@types/jest": "^27",
    "@types/node": "^16",
    "@types/uuid": "^8.3.3",
    "@typescript-eslint/eslint-plugin": "^5",
    "@typescript-eslint/parser": "^5",
    "commitlint": "^15",
    "eslint": "^8",
    "husky": "^4.3.8",
    "jest": "^27.4.5",
    "rimraf": "^3.0.2",
    "ts-jest": "^27.1.2",
    "ts-node": "^10.4.0",
    "typescript": "^4.3.4",
    "uuid": "^8.3.2"
  },
  "husky": {
    "hooks": {
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
    }
  },
  "keywords": [
    "newdash",
    "dependency injection",
    "di",
    "IoC"
  ]
}
