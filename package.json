{
  "name": "@newdash/inject",
  "version": "0.2.22",
  "main": "lib/index.js",
  "license": "MIT",
  "description": "typescript dependency inject container",
  "scripts": {
    "prebuild": "rimraf lib",
    "pretest": "eslint test/**/*.ts --fix",
    "test": "jest --detectOpenHandles --passWithNoTests",
    "build": "tsc",
    "lint": "eslint src/** --fix",
    "release": "standard-version --no-verify && git push --follow-tags origin master",
    "coverage": "npm test -- --coverage --no-cache"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/newdash/inject"
  },
  "engines": {
    "node": ">=10"
  },
  "dependencies": {
    "@newdash/newdash": "^5.13.0",
    "debug": "^4.1.1",
    "graphlib": "^2.1.8",
    "object-inspect": "^1.8.0",
    "reflect-metadata": "^0.1.13"
  },
  "devDependencies": {
    "@commitlint/config-conventional": "^11.0.0",
    "@types/debug": "^4.1.5",
    "@types/jest": "^26.0.13",
    "@types/node": "^14.6.4",
    "@types/uuid": "^8.3.0",
    "@typescript-eslint/eslint-plugin": "^4.1.0",
    "@typescript-eslint/parser": "^4.1.0",
    "commitlint": "^9.1.2",
    "eslint": "^7.8.1",
    "husky": "^4.3.0",
    "jest": "^26.4.2",
    "rimraf": "^3.0.2",
    "standard-version": "^9.0.0",
    "ts-jest": "^26.3.0",
    "ts-node": "^9.0.0",
    "typescript": "^4.0.2",
    "uuid": "^8.3.0"
  },
  "husky": {
    "hooks": {
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
    }
  },
  "keywords": [
    "newdash",
    "dependency injection",
    "di",
    "IoC"
  ]
}
